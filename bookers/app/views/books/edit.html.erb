<h1>Editing Book</h1>
<%= form_with model: @book, url: update_book_path(@book.id), method: :patch do |f| %>
  <label>Title</label><br>
  <%= f.text_field :title %><br>

  <label>Body</label><br>
  <%= f.text_area :body %><br>

  <%= f.submit 'Update Book' %>

<% end %>
<%= link_to "Edit", edit_book_path(@book.id) %>
<label>|</label>
<%= link_to "Back", "/books" %>